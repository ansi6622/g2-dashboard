<h1>Welcome to Dashboard</h1>

<% if @user %>
  Hello, <%= @user.full_name %>
  <%= button_to 'Sign Out', logout_path, action: :delete, method: "get", class: "submit_button" %>
<% else %>
  <%= link_to 'Sign In', login_path %>&nbsp;&nbsp;|&nbsp;&nbsp;<%= link_to 'Sign Up', new_user_path %>
<% end %>
<%= link_to 'View Projects', projects_path %>
<%= link_to 'About', about_path %>
<%= link_to 'Settings', settings_path %>